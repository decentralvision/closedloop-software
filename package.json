        "clean": "yarn workspace web clean",
        "preload-fonts": "yarn workspace web preload-fonts",
        "deploy-sanity": "yarn workspace studio deploy"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.0",
        "@commitlint/config-conventional": "^12.1.0",
        "husky": "^6.0.0"
    }
